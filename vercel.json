{
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist",
                "buildCommand": "pnpm install && pnpm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}

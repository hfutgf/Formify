{
    "version": 2,
    "buildCommand": "pnpm install && pnpm build",
    "installCommand": "pnpm install",
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/index.js"
      }
    ]
  }
  